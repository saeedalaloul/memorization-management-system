{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0.8",
        "ext-curl": "*",
        "adnane/simple-uuid": "^v1.0.0",
        "fruitcake/laravel-cors": "^v2.2.0",
        "guzzlehttp/guzzle": "^7.4.5",
        "ladumor/one-signal": "v0.4.1",
        "laravel/framework": "^v9.21.3",
        "laravel/jetstream": "^v2.10.1",
        "laravel/sanctum": "^v2.15.1",
        "laravel/tinker": "^v2.7.2",
        "laravelcollective/html": "^v6.3.0",
        "livewire/livewire": "^v2.10.6",
        "maatwebsite/excel": "^3.1.40",
        "spatie/laravel-backup": "^8.1.2",
        "spatie/laravel-permission": "^5.5.5",
        "yoeunes/toastr": "^v1.2.7"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^v3.6.7",
        "beyondcode/laravel-query-detector": "^1.6.0",
        "spatie/laravel-ignition": "^1.0",
        "fakerphp/faker": "^v1.19.0",
        "laravel/sail": "^v1.13.9",
        "mockery/mockery": "^1.5.0",
        "nunomaduro/collision": "^v6.2.1",
        "phpunit/phpunit": "^9.5.20"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan vendor:publish --force --tag=livewire:assets --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
